{
  "name": "work",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "workspaces": [
    "projects/*"
  ],
  "scripts": {
    "docs:dev": "cd sites/docs && vitepress dev",
    "docs:build": "ln -sf ../projects/ws/notes notes/ws && ln -sf ../projects/di/notes notes/di && cd sites/docs && vitepress build",
    "docs:preview": "cd sites/docs && vitepress preview"
  },
  "devDependencies": {
    "markdown-it-task-lists": "^2.1.1",
    "mermaid": "^11.4.0",
    "vitepress": "^1.6.4",
    "vitepress-plugin-mermaid": "^2.0.16",
    "vue": "^3.5.13"
  }
}
